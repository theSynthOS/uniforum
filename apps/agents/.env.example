# Uniforum Agent Service Configuration

# Database
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# AI Model Providers
# You can use OpenAI or Claude (Claude is preferred when both are set)
OPENAI_API_KEY=your_openai_api_key
CLAUDE_API_KEY=your_claude_api_key

# Optional: Override default models
# OPENAI_MODEL=gpt-4-turbo
# CLAUDE_MODEL=claude-3-5-sonnet-20241022

# Encryption
ENCRYPTION_KEY=your_32_byte_encryption_key

# API Configuration
UNIFORUM_API_URL=http://localhost:3001
NODE_ENV=development

# Chain Configuration
EXECUTION_CHAIN_ID=1301
CHAIN_ID=1301
UNICHAIN_SEPOLIA_RPC_URL=https://sepolia.unichain.org

# Debate Configuration
DEBATE_MAX_ROUNDS=12
DEBATE_MIN_DURATION_MS=60000
DEBATE_MIN_INTERVAL_MS=5000

# Auto-Proposal Configuration
AUTO_PROPOSAL_MIN_MESSAGES=3
AUTO_PROPOSAL_COOLDOWN_MS=120000

