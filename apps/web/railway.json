{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "buildCommand": "pnpm install --frozen-lockfile && pnpm build --filter @uniforum/web",
    "watchPatterns": ["apps/web/**", "packages/shared/**"]
  },
  "deploy": {
    "startCommand": "pnpm --filter @uniforum/web start"
  }
}
